 {
  "expo": {
    "name": "kwiyeh",
    "slug": "kwiyeh",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "kwiyeh",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.kwiyeh.kwiyeh",
      "supportsTablet": true,
      "infoPlist": {
        "NSAppTransportSecurity": {
          "NSAllowsArbitraryLoads": true
        }
      },
      "associatedDomains": [
        "applinks:kwiyeh.com"
      ],
      "config": {
        "googleMapsApiKey": "AIzaSyDeZNZtuUNXvfa99LAKVlyn08QpJpa3Nbc"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "INTERNET"
      ],
      "package": "com.kwiyeh.kwiyeh",
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "kwiyeh",
              "host": "*"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ],
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyDeZNZtuUNXvfa99LAKVlyn08QpJpa3Nbc"
        }
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyDeZNZtuUNXvfa99LAKVlyn08QpJpa3Nbc"
        }
      }
    },
    "plugins": [
      "expo-router",
      [
        "expo-build-properties",
        {
          "android": {
            "useLegacyPackaging": true
          }
        }
      ],
      [
        "expo-image-picker",
        {
          "cameraPermission": "Allow Kwiyeh to access your camera to take photos.",
          "photosPermission": "Allow Kwiyeh to access your photo library.",
          "cameraRollPermission": "Allow Kwiyeh to save photos to your device."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "98c21e6c-63df-4be6-946e-8b782ad2c35c"
      }
    }
  }
}
